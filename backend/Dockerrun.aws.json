{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "anagracetech-api",
      "image": "tblynxx94/anagracetech-api:28.02.2024.14.10.56",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 8080
        }
      ],
      "environment": [
        {
          "name" : "SPRING_DATASOURCE_URL",
          "value" : "jdbc:postgresql://db:5432/customer"
        }
      ]
    }
  ]
}